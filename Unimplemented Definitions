<Element name="Hierarchy">
<!--
Element:
	Element inherits everything from the element represented by inherit=
	name=
		By default, taken from the base element
		* means renameable
	category=
		1	One
		+	One or more
		?	Zero or one
		*	Zero or more
	name=
	id= is automatically generated by {parentName}_{name}
	inherit= id of the element to inherit attributes and subelements from
Attribute:
	name=
	valueType=
	value=
-->
